<dotnetnuke>
  <description>This is the default DotNetNuke Host Installation Template</description>
  <scripts>
    <script name="Schema">DotNetNuke.Schema</script>
    <script name="Data">DotNetNuke.Data</script>
  </scripts>
  <version>07.00.00</version>
  <installCulture>en-US</installCulture>
  <superuser>
    <firstname>SuperUser</firstname>
    <lastname>Account</lastname>
    <username>host</username>
    <password>H0tcakes</password>
    <email>host@change.me</email>
    <locale>en-US</locale>
    <updatepassword>false</updatepassword>
  </superuser>
  <connection>
    <server>server</server>
    <database>database</database>
    <integrated>false</integrated>
    <user>user</user>
    <password>password</password>
    <runasdbowner>true</runasdbowner>
    <qualifier></qualifier>
  </connection>
  <license>
    <trial></trial>
    <licenseType></licenseType>
    <webServer></webServer>
    <accountEmail></accountEmail>
    <invoiceNumber></invoiceNumber>
  </license>
  <settings>
    <AuthenticatedCacheability>2</AuthenticatedCacheability>
    <AutoAccountUnlockDuration>10</AutoAccountUnlockDuration>
    <AutoAddPortalAlias>Y</AutoAddPortalAlias>
    <CheckUpgrade>N</CheckUpgrade>
    <ControlPanel>DesktopModules/Hotcakes/ControlPanel/ControlBar.ascx</ControlPanel>
    <Copyright>N</Copyright>
	<CDNEnabled>N</CDNEnabled>
	<CrmEnableCompositeFiles>True</CrmEnableCompositeFiles>
	<CrmMinifyCss>False</CrmMinifyCss>
	<CrmMinifyJs>False</CrmMinifyJs>
    <DemoPeriod></DemoPeriod>
    <DemoSignup>N</DemoSignup>
    <DisplayBetaNotice>Y</DisplayBetaNotice>
    <EnableFileAutoSync>Y</EnableFileAutoSync>
    <EnableGettingStartedPage>False</EnableGettingStartedPage>
    <EnableContentLocalization>N</EnableContentLocalization>
	<EnableMsAjaxCDN>Y</EnableMsAjaxCDN>
    <EnableModuleOnLineHelp>N</EnableModuleOnLineHelp>
    <EnableRequestFilters>N</EnableRequestFilters>
    <EncryptionKey Secure="True"></EncryptionKey>
    <EventLogBuffer>Y</EventLogBuffer>
    <FileExtensions>swf,jpg,jpeg,jpe,gif,bmp,png,svg,ttf,eot,woff,doc,docx,xls,xlsx,ppt,pptx,pdf,txt,xml,xsl,xsd,css,zip,rar,template,htmtemplate,ico,avi,mpg,mpeg,mp3,wmv,mov,wav</FileExtensions>
    <HelpURL>http://hotcakes.org/Community</HelpURL>
    <HostCurrency>USD</HostCurrency>
    <HostEmail>support@hotcakescommerce.com</HostEmail>
    <HostFee></HostFee>
    <HostPortalId>0</HostPortalId>
    <HostSpace></HostSpace>
    <HostTitle>Hotcakes Commerce</HostTitle>
    <HostURL>https://hotcakes.org</HostURL>
    <HttpCompression>0</HttpCompression>
    <HttpCompressionLevel>0</HttpCompressionLevel>
    <ModuleCaching>MemoryModuleCachingProvider</ModuleCaching>
    <PageQuota></PageQuota>
    <PageStatePersister>P</PageStatePersister>
    <PaymentProcessor>PayPal</PaymentProcessor>
    <PerformanceSetting>6</PerformanceSetting>
    <ProcessorPassword Secure="True"></ProcessorPassword>
    <ProcessorUserId Secure="True"></ProcessorUserId>
    <ProxyPassword Secure="True"></ProxyPassword>
    <ProxyPort></ProxyPort>
    <ProxyServer></ProxyServer>
    <ProxyUsername Secure="True"></ProxyUsername>
    <SchedulerMode>1</SchedulerMode>
    <SiteLogBuffer>1</SiteLogBuffer>
    <SiteLogHistory>0</SiteLogHistory>
    <SiteLogStorage>D</SiteLogStorage>
    <SkinUpload>G</SkinUpload>
    <SMTPAuthentication></SMTPAuthentication>
    <SMTPEnableSSL>N</SMTPEnableSSL>
    <SMTPPassword Secure="True"></SMTPPassword>
    <SMTPServer>smtp.appliedi.net</SMTPServer>
    <SMTPUsername Secure="True"></SMTPUsername>
    <UseCustomErrorMessages>Y</UseCustomErrorMessages>
    <UseFriendlyUrls>Y</UseFriendlyUrls>
    <UserQuota></UserQuota>
    <UsersOnlineTime>20</UsersOnlineTime>
    <WebRequestTimeout></WebRequestTimeout>
    <WhitespaceFilter>N</WhitespaceFilter>
  </settings>
  <desktopmodules/>
  <portals>
    <portal>
      <portalname>My E-Commerce CMS</portalname>
      <administrator>
        <firstname>Administrator</firstname>
        <lastname>Account</lastname>
        <username>admin</username>
        <password>H0tcakes</password>
        <email>admin@change.me</email>
      </administrator>
      <description>My Website</description>
      <keywords>DotNetNuke, DNN, Content, Management, CMS</keywords>
      <templatefile>HCHostedTrial.template</templatefile>
      <portalaliases>
        <portalalias></portalalias>
      </portalaliases>
      <ischild>false</ischild>
    </portal>
  </portals>
</dotnetnuke>
