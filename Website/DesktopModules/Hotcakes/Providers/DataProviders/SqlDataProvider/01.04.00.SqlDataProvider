ALTER TABLE {databaseOwner}[{objectQualifier}hcc_Address]
DROP COLUMN [CountyBvin], [CountyName]
GO

update {databaseOwner}[{objectQualifier}hcc_StoreSettings]
set [SettingValue] = Upper([SettingValue])
where [SettingName] = 'DisabledCountryIso3Codes'
GO

update {databaseOwner}[{objectQualifier}hcc_Taxes]
set [RegionName] = ''
where [RegionName] = '_'
GO

update {databaseOwner}[{objectQualifier}hcc_Taxes]
set [CountryName] = (select [IsoAlpha3] from {databaseOwner}[{objectQualifier}hcc_Countries] where [SystemName] = [CountryName] )
GO



/* Localization of text product type properties */

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_ProductProperty] ADD
	[IsLocalizable] bit NOT NULL CONSTRAINT [DF_{objectQualifier}hcc_ProductProperty_IsLocalizable] DEFAULT 0
GO
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_ProductPropertyTranslations] ADD
	[DefaultLocalizableValue] nvarchar(MAX) NULL
GO

CREATE TABLE {databaseOwner}[{objectQualifier}hcc_ProductPropertyValueTranslations](
	[ProductPropertyValueTranslationId] [bigint] IDENTITY(1,1) NOT NULL,
	[ProductPropertyValueId] [bigint] NOT NULL,
	[Culture] [nvarchar](10) NOT NULL,
	[PropertyLocalizableValue] [nvarchar](max) NULL,
 CONSTRAINT [PK_{objectQualifier}hcc_ProductPropertyValueTranslations] PRIMARY KEY CLUSTERED 
(
	[ProductPropertyValueTranslationId] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON),
 CONSTRAINT [IX_{objectQualifier}hcc_ProductPropertyValueTranslations] UNIQUE NONCLUSTERED 
(
	[ProductPropertyValueId] ASC,
	[Culture] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON) 
)

GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_ProductPropertyValueTranslations]  WITH CHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_ProductPropertyValueTranslations_hcc_ProductPropertyValue] FOREIGN KEY([ProductPropertyValueId])
REFERENCES {databaseOwner}[{objectQualifier}hcc_ProductPropertyValue] ([Id])
ON DELETE CASCADE
GO
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_ProductPropertyValueTranslations] CHECK CONSTRAINT [FK_{objectQualifier}hcc_ProductPropertyValueTranslations_hcc_ProductPropertyValue]
GO

/* Removed magic string for logos */
update {databaseOwner}[{objectQualifier}hcc_StoreSettings]
set [SettingValue] = ''
where [SettingName] = 'LogoImage'
	and [SettingValue] = '[[default]]'
GO
