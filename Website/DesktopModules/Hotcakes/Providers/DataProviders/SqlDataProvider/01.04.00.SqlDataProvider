/* Managment anc localization of countries and regions */
CREATE TABLE {databaseOwner}[{objectQualifier}hcc_Countries](
	[CountryId] [uniqueidentifier] NOT NULL,
	[CultureCode] [nvarchar](10) NULL,
	[SystemName] [nvarchar](255) NULL,
	[IsoCode] [nvarchar](10) NULL,
	[IsoAlpha3] [nvarchar](10) NULL,
	[IsoNumeric] [nvarchar](10) NULL,
	[PostalCodeValidationRegex] [nvarchar](10) NULL,
 CONSTRAINT [PK_{objectQualifier}hcc_Countries] PRIMARY KEY CLUSTERED 
(
	[CountryId] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON)
)
GO

CREATE TABLE {databaseOwner}[{objectQualifier}hcc_Regions](
	[RegionId] [uniqueidentifier] NOT NULL,
	[CountryId] [uniqueidentifier] NOT NULL,
	[Abbreviation] [nvarchar](10) NULL,
	[SystemName] [nvarchar](255) NULL,
 CONSTRAINT [PK_{objectQualifier}hcc_Regions] PRIMARY KEY CLUSTERED 
(
	[RegionId] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON)
)
GO

CREATE TABLE {databaseOwner}[{objectQualifier}hcc_CountryTranslations](
	[CountryTranslationId] [bigint] IDENTITY(1,1) NOT NULL,
	[CountryId] [uniqueidentifier] NOT NULL,
	[Culture] [nvarchar](10) NOT NULL,
	[DisplayName] [nvarchar](255) NULL,
 CONSTRAINT [PK_{objectQualifier}hcc_CountryTranslations] PRIMARY KEY CLUSTERED 
(
	[CountryTranslationId] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON),
 CONSTRAINT [IX_{objectQualifier}hcc_CountryTranslations] UNIQUE NONCLUSTERED 
(
	[CountryId] ASC,
	[Culture] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON)
)
GO

CREATE TABLE {databaseOwner}[{objectQualifier}hcc_RegionTranslations](
	[RegionTranslationId] [bigint] IDENTITY(1,1) NOT NULL,
	[RegionId] [uniqueidentifier] NOT NULL,
	[Culture] [nvarchar](10) NOT NULL,
	[DisplayName] [nvarchar](255) NULL,
 CONSTRAINT [PK_{objectQualifier}hcc_RegionTranslations] PRIMARY KEY CLUSTERED 
(
	[RegionTranslationId] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON),
 CONSTRAINT [IX_{objectQualifier}hcc_RegionTranslations] UNIQUE NONCLUSTERED 
(
	[RegionId] ASC,
	[Culture] ASC
)WITH (PAD_INDEX  = OFF, STATISTICS_NORECOMPUTE  = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS  = ON, ALLOW_PAGE_LOCKS  = ON)
)
GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_Countries] ADD  CONSTRAINT [DF_{objectQualifier}hcc_Countries_CountryId]  DEFAULT (newid()) FOR [CountryId]
GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_Regions] ADD  CONSTRAINT [DF_{objectQualifier}hcc_Regions_RegionId]  DEFAULT (newid()) FOR [RegionId]
GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_CountryTranslations]  WITH CHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_CountryTranslations_hcc_Countries] FOREIGN KEY([CountryId])
REFERENCES {databaseOwner}[{objectQualifier}hcc_Countries] ([CountryId])
ON DELETE CASCADE
GO
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_CountryTranslations] CHECK CONSTRAINT [FK_{objectQualifier}hcc_CountryTranslations_hcc_Countries]
GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_Regions]  WITH CHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_Regions_hcc_Countries] FOREIGN KEY([CountryId])
REFERENCES {databaseOwner}[{objectQualifier}hcc_Countries] ([CountryId])
ON DELETE CASCADE
GO
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_Regions] CHECK CONSTRAINT [FK_{objectQualifier}hcc_Regions_hcc_Countries]
GO

ALTER TABLE {databaseOwner}[{objectQualifier}hcc_RegionTranslations]  WITH CHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_RegionTranslations_hcc_Regions] FOREIGN KEY([RegionId])
REFERENCES {databaseOwner}[{objectQualifier}hcc_Regions] ([RegionId])
ON DELETE CASCADE
GO
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_RegionTranslations] CHECK CONSTRAINT [FK_{objectQualifier}hcc_RegionTranslations_hcc_Regions]
GO

