IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}hcc_GenerateOrderNumber]') AND type in (N'P', N'PC'))
BEGIN
EXEC sp_executesql @statement = N'CREATE PROCEDURE {databaseOwner}[{objectQualifier}hcc_GenerateOrderNumber]

@storeid bigint

AS

	BEGIN TRY		
		BEGIN TRAN
		
		DECLARE @OrderNumberSeed bigint
		DECLARE @MaxOrderNumber bigint
			
		SET @OrderNumberSeed = (SELECT CAST(COALESCE([SettingValue], 0) AS bigint) 
			FROM {databaseOwner}[{objectQualifier}hcc_StoreSettings] WITH (UPDLOCK) WHERE [SettingName] = ''LastOrderNumber''
			AND StoreId=@storeid)
				
		--SET @MaxOrderNumber = (SELECT COALESCE(MAX([OrderNumber]), -1) FROM {databaseOwner}[{objectQualifier}hcc_Order])
		--IF @OrderNumberSeed < @MaxOrderNumber
		--		SET @OrderNumberSeed = @MaxOrderNumber
				
		SET @OrderNumberSeed = @OrderNumberSeed + 1
		
		UPDATE {databaseOwner}[{objectQualifier}hcc_StoreSettings] SET [SettingValue] = @OrderNumberSeed 
			WHERE [SettingName] = ''LastOrderNumber''
			AND [StoreId] = @storeid
		
		COMMIT
		
		SELECT @OrderNumberSeed	AS [OrderNumber]	
	END TRY
	BEGIN CATCH
		ROLLBACK
		--EXEC {databaseOwner}[{objectQualifier}hcc_EventLog_SQL_i]
	END CATCH

' 
END
GO
IF NOT EXISTS (SELECT * FROM sys.foreign_keys WHERE object_id = OBJECT_ID(N'{databaseOwner}[FK_{objectQualifier}hcc_AuthTokens_hcc_UserAccounts]') AND parent_object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}hcc_AuthTokens]'))
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_AuthTokens]  WITH CHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_AuthTokens_hcc_UserAccounts] FOREIGN KEY([UserId])
REFERENCES {databaseOwner}[{objectQualifier}hcc_UserAccounts] ([Id])
GO
IF  EXISTS (SELECT * FROM sys.foreign_keys WHERE object_id = OBJECT_ID(N'{databaseOwner}[FK_{objectQualifier}hcc_AuthTokens_hcc_UserAccounts]') AND parent_object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}hcc_AuthTokens]'))
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_AuthTokens] CHECK CONSTRAINT [FK_{objectQualifier}hcc_AuthTokens_hcc_UserAccounts]
GO
IF NOT EXISTS (SELECT * FROM sys.foreign_keys WHERE object_id = OBJECT_ID(N'{databaseOwner}[FK_{objectQualifier}hcc_ContentBlock_hcc_ContentColumn]') AND parent_object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}hcc_ContentBlock]'))
ALTER TABLE {databaseOwner}[{objectQualifier}hcc_ContentBlock]  WITH NOCHECK ADD  CONSTRAINT [FK_{objectQualifier}hcc_ContentBlock_hcc_ContentColumn] FOREIGN KEY([ColumnID])
